docker login docker.pkg.github.com -u benthayer -p $GITHUB_TOKEN

docker pull docker.pkg.github.com/thomasdriscoll/open-source-illinois-web/frontend:latest -t new-frontend
docker pull docker.pkg.github.com/thomasdriscoll/open-source-illinois-web/backend:latest -t new-backend

docker stop frontend
docker stop backend

docker rmi frontend
docker rmi backend

docker tag new-frontend frontend
docker tag new-backend backend

docker run frontend
docker run backend

